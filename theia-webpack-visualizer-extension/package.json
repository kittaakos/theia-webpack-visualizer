{
  "name": "theia-webpack-visualizer-extension",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.0.0",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "latest",
    "@types/express": "^4.0.36",
    "@theia/filesystem": "latest",
    "@theia/messages": "latest",
    "@theia/navigator": "latest",
    "@types/webpack": "^2.2.1",
    "express": "^4.15.3",
    "webpack": "^2.2.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2",
    "ts-node": "^4.0.1",
    "typescript": "latest"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "mocha -t 20000ms --require ts-node/register --require reflect-metadata/Reflect --watch-extensions ts \"./src/**/*.*spec.ts\""
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/theia-webpack-visualizer-frontend-module",
      "backend": "lib/node/theia-webpack-visualizer-backend-module"
    }
  ]
}